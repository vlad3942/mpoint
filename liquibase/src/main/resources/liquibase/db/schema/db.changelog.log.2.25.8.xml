<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <!-- Rename column in transaction_tbl  -->
    <changeSet author="Priya Alamwar" id="2021-02-11-master-v2.25.7">
        <preConditions onFail="MARK_RAN">
            <columnExists catalogName="log"
                          schemaName= "log"
                          tableName="transaction_tbl"
                          columnName="exchangeinfoid" />
        </preConditions>

        <renameColumn catalogName="log"
                      schemaName= "log"
                      tableName="transaction_tbl"
                      oldColumnName="exchangeinfoid"
                      newColumnName="fxservicetypeid" />
    </changeSet>
</databaseChangeLog>